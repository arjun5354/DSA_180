	int print2largest(int arr[], int n) {
	   int maxi=INT_MIN;
	   for(int i=0;i<n;i++){
	       if(arr[i]>maxi){
	           maxi=arr[i];
	       }
	   }
	   
	   int smaxi=-1;
	   for(int i=0;i<n;i++){
	       if(arr[i]>smaxi && arr[i]!=maxi){
	           smaxi=arr[i];
	       }
	   }
	   return smaxi;
	}
