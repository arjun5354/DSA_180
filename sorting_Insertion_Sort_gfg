    void swap(int* a , int* b){
        int temp=*a;
        *a=*b;
        *b=temp;
    }
    void insertionSort(int arr[], int n)
    {
        for(int i=1;i<n;i++){
            for(int j=i;j>0;j--){
                if(arr[j]<arr[j-1]){
                        // int temp=arr[j];
                        // arr[j]=arr[j-1];
                        // arr[j-1]=temp;
                        swap(&arr[j],&arr[j-1]);
                }
            }
        }
    }
